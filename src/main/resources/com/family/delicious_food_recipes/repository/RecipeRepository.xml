<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.family.delicious_food_recipes.repository.RecipeRepository">

    <resultMap type="com.family.delicious_food_recipes.model.RecipeModel" id="getRecipeListResult">
        <result property="id" column="id" />
        <result property="name" column="name" />
        <result property="cost" column="cost" />
        <result property="calorie" column="calorie" />
    </resultMap>

    <select id="getRecipeList" parameterType="java.lang.String" resultMap="getRecipeListResult">
        SELECT *
        FROM recipe;
    </select>

</mapper>