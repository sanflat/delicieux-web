<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.family.delicious_food_recipes.repository.SeasonRepository">

    <select id="season_list" parameterType="java.lang.String" resultMap="season_list_result">
        SELECT *
        FROM season;
    </select>
    <resultMap type="com.family.delicious_food_recipes.model.SeasonModel" id="season_list_result">
        <result property="id" column="id" />
        <result property="name" column="name" />
    </resultMap>

    <insert id="season_post" parameterType="com.family.delicious_food_recipes.model.SeasonModel">
        INSERT INTO  season(
        name
        ) VALUES (
        #{name}
        )
    </insert>

</mapper>